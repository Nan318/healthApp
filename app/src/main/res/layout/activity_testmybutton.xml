<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/qolChartTopBtn2"
    android:orientation="vertical"
    tools:context=".TestMyButton">

    <com.example.zhongzhoujianshe.healthapp.MyRoundCornerButton
        android:id="@+id/aaa"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_margin="16dp"
        android:text="balabala" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:orientation="horizontal">

        <com.example.zhongzhoujianshe.healthapp.MyRoundCornerButton
            android:id="@+id/btn1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingBottom="5dp"
            android:paddingTop="5dp"
            android:text="com.example.zhongzhoujianshe.healthapp.MyRoundCornerButton" />

        <com.example.zhongzhoujianshe.healthapp.MyRoundCornerButton
            android:id="@+id/btn2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingBottom="5dp"
            android:paddingTop="5dp"
            android:text="com.example.zhongzhoujianshe.healthapp.MyRoundCornerButton" />

        <com.example.zhongzhoujianshe.healthapp.MyRoundCornerButton
            android:id="@+id/btn3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingBottom="5dp"
            android:paddingTop="5dp"
            android:text="com.example.zhongzhoujianshe.healthapp.MyRoundCornerButton" />
    </LinearLayout>

</LinearLayout>